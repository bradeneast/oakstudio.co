{
  "netlify.site_id": "oakstudio.netlify.com",
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".css",
      "savePath": "/_src"
    }
  ],
  "liveServer.settings.root": "/_site",
  "saveAndRun": {
    "commands": [
      {
        "cmd": "esbuild _src/_includes/js/main.js --outfile=_src/_includes/js/main.min.js --minify --bundle",
        "match": "_includes/*.js"
      }
    ]
  }
}